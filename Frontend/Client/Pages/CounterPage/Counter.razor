@page "/counter"
@page "/t/{T:int}/counter"

@inject NavigationManager NavMan

@inherits CounterModel

<PageTitle>Counter</PageTitle>

<h1>Counter</h1>

<p role="status">Current count: @(currentCount)</p>

<button class="btn btn-primary" @onclick="() => Count(CounterActions.COUNTER_INC)">Inc</button>
<button class="btn btn-primary" @onclick="() => Count(CounterActions.COUNTER_DEC)">Dec</button>

